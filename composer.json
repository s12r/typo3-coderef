{
  "name": "somebdyelse/coderef",
  "description": "TYPO3 Extension that allows to reference database records in the site configuration",
  "authors": [
    {
      "name": "Hannes Lau",
      "email": "office@hanneslau.de"
    }
  ],
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "^10.4.0 || ^11.5.0 || ^12.4.0"
  },
  "autoload": {
    "psr-4": {
      "SomeBdyElse\\Coderef\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "coderef"
    }
  }
}
